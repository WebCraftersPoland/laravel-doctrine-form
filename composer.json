{
    "name": "webcrafters/laravel-doctrine-form",
    "description": "LaravelCollective Form builder, extended for usage with Laravel Doctrine",
    "license": "MIT",
    "homepage": "https://webcrafters.pl",
    "authors": [
        {
            "name": "Kamil Kawka-Tomczak",
            "email": "kamil.kawka-tomczak@webcrafters.pl"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "laravelcollective/html": "5.6.*"
    },
    "autoload": {
        "psr-4": {
            "WebCrafters\\LaravelDoctrineForm\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "WebCrafters\\LaravelDoctrineForm\\HtmlServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
